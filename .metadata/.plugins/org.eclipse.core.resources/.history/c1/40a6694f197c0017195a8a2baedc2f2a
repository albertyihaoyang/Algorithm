package com.yyh;

import java.util.*;

class ListNode {
	int val;
	ListNode next;
	ListNode(int x) { val = x; }
}

public class MergeKLists {

	public static void main(String[] args) {
		MergeKLists my = new MergeKLists();
		ListNode L1 = new ListNode(1);
		ListNode[] l1 = new ListNode[]{L1};
		System.out.println(my.mergeKLists(l1).val);
	}

	public ListNode mergeKLists(ListNode[] lists) {
        ListNode dummy = new ListNode(0);
        ListNode cur = dummy;
        List<ListNode> list = Arrays.asList(lists);
        boolean flag = true;
		while (flag){
			flag = false;
			int minIdx = -1;
			for (int i = 0; i < list.size(); i++){
				flag = true;
				if (minIdx == -1){
					minIdx = i;
				} else if (list.get(minIdx).val > list.get(i).val){
					minIdx = i;
				}
			}
			if (minIdx != -1){
				cur.next = list.get(minIdx);
				if (list.get(minIdx).next != null){
					list.add(list.get(minIdx));
				}
				list.remove(minIdx);
				cur = cur.next;
			}
        }
		return dummy.next;
    }
}
